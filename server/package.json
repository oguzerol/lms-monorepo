{
  "name": "lms-backend",
  "version": "1.2.0",
  "description": "Learning Management System Backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "lint": "eslint --fix src",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
